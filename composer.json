{
  "name": "garlic/graphql-wrapper",
  "description": "GraphQL wrapper bundle uses for communication among microservices through message bus (by default is RabbitMQ)",
  "keywords": ["Wrapper", "GraphQL", "Microservices"],
  "license": "MIT",
  "authors": [
    {
      "name": "Maksym Churkyn",
      "email": "imaximius@gmail.com"
    }
  ],
  "type": "symfony-bundle",
  "autoload": {
    "psr-4": {
      "Garlic\\Wrapper\\": ""
    }
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/egrik/helpers.git"
    }
  ],
  "require": {
    "php": ">=7.1",
    "garlic/bus": "1.*",
    "jms/serializer-bundle": "^2.0",
    "dflydev/dot-access-data": "^2.0",
    "ext-json": "*",
    "garlic/graphql": "^1.2",
    "garlic/helpers": "^1.0"
  }
}
